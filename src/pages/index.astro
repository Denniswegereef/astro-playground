---
import Layout from "../layouts/Layout.astro";
import Link from "../components/link/link.astro";

const routes = [
  "/404.html",
  "/base-three-extended",
  "/base-three",
  "base-p5",
  "p5-hello-color",
  "p5-color-spectrum-grid",
  "p5-color-spectrum-circle",
  "/index",
  // "/jonsson-slider",
  // "/kadena-cone-animation",
  // "/kadena-cone",
  // "/kadena-cube",
  // "/kadena-scroll-animation-player-shader",
  // "/kadena-scroll-animation-player-matcap",
  // "/resonating-letters-overlay",
  // "/resonating-letters-v1",
  // "/resonating-letters-v2",
  // "/scroll-trigger-resonating-letters",
  // "/msdf-font",
  // "/kadena-sphere",
  // "/kadena-sequence",
  // "/three-scroll-tracking"
]

---

<Layout title="Welcome to Astro.">
  <main>
    <ul>
      {
        routes.map((route) => (
          <li>
            <Link href={route} label={route} />
          </li>
        ))
      }
    </ul>
  </main>
</Layout>
